  },
  "keywords": [
    "aws",
    "cloud",
    "security",
    "reports",
    "scans"
  ],
  "author": "dilip@tensult.com",
  "license": "MIT",
 "dependencies": {
    "aws-sdk": "^2.1565.0",
    "axios": "^1.7.4",
    "cfn-response-promise": "^1.1.0",
    "urldecode": "^1.0.1"
  }
}
