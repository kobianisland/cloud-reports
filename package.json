{
  "name": "cloud-reports",
  "version": "1.6.4",
  "description": "Collects and analyzes cloud resources",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm install && npm run copy-ejs && tsc && cd src/reporters/html && npm install && npm run ng build --  --prod && cd ../../..",
